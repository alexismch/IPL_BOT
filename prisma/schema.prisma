generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id      String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  guildId String
  userId  String
  email   String
}

model Series {
  id             String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  guildId        String
  block          Int
  seriesNumber   Int
  roleId         String
  textChannelId  String
  voiceChannelId String

  @@unique([guildId, block, seriesNumber])
}
